<p id="notice"><%= notice %></p>

<h1>Inventories</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Price</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @inventories.each do |inventory| %>
      <tr>
        <td><%= inventory.name %></td>
        <td><%= inventory.price %></td>
        <td><%= link_to 'Show', venue_inventory_path(@venue, inventory)%></td>
        <td><%= link_to 'Edit', edit_venue_inventory_path(@venue,inventory) %></td>
        <td><%= link_to 'Destroy', venue_inventory_path(@venue,inventory), method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Inventory', new_venue_inventory_path %>
<%= link_to 'venues',  venues_path %>
<%= link_to 'All_Category',  categories_path %>